<script>

	function validateFname()
	{
		var fname =document.getElementById("fname");
		var regex=/^[a-zA-Z]+$/;
		if(!fname.value.match(regex)){
			document.getElementById("ferr").innerHTML="The name entered is invalid";
			fname.focus();
			console.log("wrong fname");
			return false
		}
		else
		{
			document.getElementById("ferr").innerHTML="";
		}


	}

	function validateLname()
	{
		var fname =document.getElementById("lname");
		var regex=/^[a-zA-Z]+$/;
		if(!fname.value.match(regex)){
			document.getElementById("lerr").innerHTML="The name entered is invalid";
			fname.focus();
			console.log("wrong lname");
			return false
		}
		else
		{
			document.getElementById("lerr").innerHTML="";
		}


	}
	function validatePhone()
	{
		var regNumber =/^(\+\d{1,3}[- ]?)?\d{10}$/;
		var phone =document.getElementById("phone");
		if(!phone.value.match(regNumber))
		{

			document.getElementById("perr").innerHTML="Number is invalid";
			phone.focus();
			return false;
		}
		else{

			document.getElementById("perr").innerHTML="";
		}


	}
	
	function validate()
	{
		var fname =document.getElementById("fname");
		var lname =document.getElementById("lname");
		//var mail =document.getElementById("email");
		var phone =document.getElementById("phone");
		var regex=/^[a-zA-Z]+$/;
		if(!fname.value.match(regex)){
			document.getElementById("ferr").innerHTML="The name entered is invalid";
			fname.focus();
			console.log("wrong fname");
			return false
		}
		else
		{
			document.getElementById("ferr").innerHTML="";
		}
		if(!lname.value.match(regex))
		{
			lname.focus();
			return false;
		}

		var regNumber =/^(\+\d{1,3}[- ]?)?\d{10}$/;
		if(!phone.value.match(regNumber))
		{
			phone.focus();
			return false;
		}


		return true;

	}

</script>